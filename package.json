{
  "name": "velvet-style",
  "version": "0.2.1",
  "description": "Simple Vanilla JavaScript Universal CSS in JS library",
  "type": "module",
  "main": "index.js",
  "module": "index.mjs",
  "exports": {
    "import": "index.mjs",
    "require": "index.cjs"
  },
  "types": "index.d.ts",
  "scripts": {
    "test": "jest --coverage",
    "build": "rimraf build rimraf index.* dist && tsc && rollup --config",
    "ts:check": "tsc --noEmit",
    "prepublishOnly": "npm run ts:check && npm run build"
  },
  "keywords": [
    "css",
    "css-in-js",
    "style"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jcubic/velvet.git"
  },
  "bugs": {
    "url": "https://github.com/jcubic/velvet/issues"
  },
  "homepage": "https://github.com/jcubic/velvet#readme",
  "author": "Jakub T. Jankiewicz <jcubic@onet.pl> (https://jcubic.pl/me/)",
  "license": "MIT",
  "devDependencies": {
    "@parcel/packager-ts": "^2.10.3",
    "@parcel/transformer-typescript-types": "^2.10.3",
    "@rollup/plugin-typescript": "^11.1.5",
    "@types/cssom": "^0.4.3",
    "@types/jest": "^29.5.11",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "rimraf": "^5.0.5",
    "rollup": "^4.9.1",
    "rollup-plugin-dts": "^6.1.0",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3"
  },
  "dependencies": {
    "cssom": "^0.5.0"
  }
}
